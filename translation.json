{
  "Test-MacOS-MacMini": {
    "Build": {
      "build_myapp": ["bazel build --verbose_failures --compilation_mode=dbg --config=symbols", "//apps/myapp:myapp"],
      "build_myapp_portable": ["bazel build --verbose_failures --compilation_mode=dbg --config=symbols", "//apps/myapp:myapp_portable"],
      "build_draw:draw": ["bazel build --verbose_failures --compilation_mode=dbg --config=symbols", "//apps/draw:draw"],
      "build_ray:apps": ["bazel build --verbose_failures --compilation_mode=dbg --config=symbols", "//ray/apps/..."],
      "build_tools:cskiaapp": ["bazel build --verbose_failures --compilation_mode=dbg --config=symbols", "//tools:cskiaapp"]
    },
    "Run": {
      "run_tools:tests": ["bazel run --verbose_failures --compilation_mode=dbg --config=symbols", "//tools:tests"],
      "run_ray:unittests": ["bazel test --verbose_failures --compilation_mode=dbg --config=symbols", "//ray:unittests"],
      "run_tools:rasterapp": ["bazel run --verbose_failures --compilation_mode=dbg --config=symbols", "//tools:rasterapp"],
      "run_gm": ["bazel run --verbose_failures --compilation_mode=dbg --config=symbols", "//gm:gmapp -- -o ./temp"],
      "run_wrapper_smart_ptrs:unittests": ["bazel test --verbose_failures --compilation_mode=dbg --config=symbols", "//src/wrapper_smart_ptrs:unittests"],
      "run_tools:bench": ["bazel run --verbose_failures --compilation_mode=opt --config=symbols", "//tools:bench"],
      "run_ray:rasterizer_bench": ["bazel run --verbose_failures --compilation_mode=opt --config=symbols", "//ray/drawlib:rasterizer_bench"],
    }
    "Builder": "work-macOS-test-test-test-test-test"
  }
}
